{
  "name": "com.hackday.dinningshare",
  "displayName": "DinningShare",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "start": "yarn run clean && yarn run copy && webpack && webpack-dev-server --inline --hot --port 8088",
    "build": "webpack && rm -rf platforms/android/assets && cordova run android",
    "clean": "rm -rf www/images",
    "copy": "cp -R src/images www/"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "classnames": "^2.2.5",
    "cordova-android": "^6.2.3",
    "cordova-plugin-whitelist": "^1.3.2",
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^2.5.2",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.26.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "jest": "^18.1.0",
    "node-sass": "^4.3.0",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "sass-loader": "^4.1.1",
    "snapsvg": "^0.5.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-stream": "^3.2.0",
    "material-ui": "^0.18.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  }
}
